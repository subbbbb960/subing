package com.sd.turtle;
import ch.aplu.turtle.*;
import java.awt.Color;

public class WeightTurtle extends Turtle {
    private int weight;
    public WeigthTurtle(int w){
      weight=w;
      setPenColor(Color,red);
    }
    public int getWeight(){
      return weight;
    }
}